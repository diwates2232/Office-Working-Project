const { fetchRegionData, fetchGlobalSummary } = require("../services/excelService");

// Controller to handle global summary
const getGlobalSummary = async (req, res) => {
    try {
        const summary = await fetchGlobalSummary();
        res.status(200).json(summary);
    } catch (error) {
        res.status(500).json({ error: "Internal server error", details: error.message });
    }
};

module.exports = { getRegionDetails, getGlobalSummary };
